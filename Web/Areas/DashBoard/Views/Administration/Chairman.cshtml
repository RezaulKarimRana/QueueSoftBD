@model DashBoardVM;

<div id="contents" class="sixteen columns">
    <div class="column block mb-4" style="text-align:center">
        <p style="text-align:center">
            <img alt="Principal" src="@Model.ChairmanImage" style="border-style:solid; border-width:0px; height:176px; margin-left:40px; margin-right:40px; width:150px" />
        </p>
        <h3>@Model.ChairmanName</h3>
        <h5>সভাপতি, গভর্নিং বডি, @Model.InstituteName</h5>
        <p>@Model.ChairmanSpeech</p>
    </div>
</div>